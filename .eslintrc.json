{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "func-names": "off",
    "linebreak-style": ["error", "windows"],
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "sometimes"
      }
    ],
    "max-len": [
      "error",
      {
        "code": 100,
        "ignoreComments": true
      }
    ]
  }
}